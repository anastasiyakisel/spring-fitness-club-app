<html xmlns:jsp="http://java.sun.com/JSP/Page" 
	xmlns:form="http://www.springframework.org/tags/form" 
	xmlns:c="http://java.sun.com/jsp/jstl/core" 
	xmlns:sf="http://www.springframework.org/tags/form" 
	xmlns:spring="http://www.springframework.org/tags">
<jsp:output omit-xml-declaration="yes" />
<jsp:directive.page contentType="text/html;charset=UTF-8" />

<head>
	<title><spring:message code="tittle.registration" /></title>
	
	<script type="text/javascript" src="js/jquery-1.11.3.min.js"><jsp:text/></script>
	<c:url var="pageCssUrl" value="/css/pageStyle.css" />
	<c:url var="headerCssUrl" value="/css/logoStyle.css" />
	<c:url var="url" value="/css/registrationStyle.css" />
	
	<link href="${pageCssUrl}" rel="stylesheet" type="text/css" />
	<link href="${headerCssUrl}" rel="stylesheet" type="text/css" />
	<link href="${url}" rel="stylesheet" type="text/css" />
	
	<meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />

</head>
<body>

<div id="pageReg">
	<div id="sticky-footer-wrap">
		<header class="site-header">
					
			   <div id="headerWrapper">
					<div id="logoWrapper">
						<p id="stars">
							<span>&#9733;</span><span>&#9733;</span><span>&#9733;</span><span>&#9733;</span><span>&#9733;</span>
						</p>

						<p id="title" contenteditable="true" spellcheck="false">
							<span>100</span> <span><spring:message code="letter.1" /></span>
							<span><spring:message code="letter.2" /></span> <span><spring:message
									code="letter.3" /></span> <span><spring:message
									code="letter.4" /></span> <span><spring:message
									code="letter.5" /></span>
						</p>
						<p id="slogan">
							<a href="${flowExecutionUrl}&amp;_eventId=returnHome">
								Fitness Club Home Page
							</a>
						</p>
					</div>

					<div id="localeMenu">
						<a href="?locale=en_US"><img alt="usa" src="images/usa.png" width="32" height="32" /></a> 
						<a href="?locale=ru_RU"><img alt="ru" src="images/ru.png" width="32" height="32" /></a>
					</div>

				</div>

		</header>
			<div class="site-main">
				<div id="primary">
							<div class="main">
		
					 	<div class="register">
					
					 		<h3><spring:message code="caption.newaccount" /></h3>
							<sf:form commandName="user" class="register" action="${flowExecutionUrl}&amp;_eventId_newUsernameEntered">
								<input type="hidden" name="_flowExecutionKey" value="${flowExecutionKey}"/>
								<div class="required">
									<sf:label path="firstName" class="label">
										<spring:message code="signature.firstname" />
									</sf:label>
									<sf:input path="firstName" type="text" spellcheck="false" placeholder="Michael" />
								</div>
					
								<div class="required">
									<sf:label path="lastName" class="label">
										<spring:message code="signature.lastname" />
									</sf:label>
					
									<sf:input path="lastName" type="text" spellcheck="false" placeholder="Jordan"/>
					
								</div>
					
					
								<div class="required">
									<sf:label path="address" class="label">
										<spring:message code="signature.address" />
									</sf:label >
									<sf:input path="address" type="text" spellcheck="false" placeholder="228 Park Ave S NY"/>
								</div>
					
					
								<div class="required">
									<sf:label path="telephone" class="label">
									<spring:message code="signature.telephone" />
									</sf:label>
									<sf:input path="telephone" type="text" spellcheck="false" placeholder="(212) 696-5268"/>
								</div>
					
					
								<div>
									<sf:label path="description" class="label">
									<spring:message code="signature.description" />
									</sf:label>
									<sf:input path="description" type="text" spellcheck="false" placeholder="Interested in basketball."/>
								</div>
					
					
								<div class="required">
									<sf:label path="login" class="label">
										<spring:message code="signature.login" />
									</sf:label>
									<sf:input path="login" />
								</div>
					
					
								<div class="required">
									<sf:label path="password" class="label">
										<spring:message code="signature.pass" />
									</sf:label>
									<sf:input path="password" type="password"/>
								</div>
								
								<div>
									<sf:label path=""/>
									<spring:message code="label.registration" var="submit" />
									<input type="submit" value="${submit}" class="button" />
								</div>
					</sf:form>
					</div>
					</div> 

				</div>	        	
				<div id="secondary">
					<c:if test="${not empty errorMessage}">
						<h3><spring:message code="${errorMessage}" /></h3>
					</c:if>
				</div>
				<div id="tertiary">
					<h3></h3>
				</div>
			</div>
		</div>
</div>

<footer class="site-footer">
		<p class="footer">
			&#169; 2015 KISEL 
		</p>
</footer>  
</body>
</html>